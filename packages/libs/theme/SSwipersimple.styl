#box
  position:relative
  width:84%
  height:200px
  margin 30px auto
  .swiper-side
    position:absolute
    left:0px
    top:0px
    width:100%
    height:200px
    z-index:0
   img
      width:100%
      height 100%
  .swiper-side:nth-child(1)
    z-index:1
  ul
    position:absolute
    right:50px
    bottom:10px
    list-style:none
    z-index:2
  ul li
    float:left
    margin-left:20px
    width:15px
    height:15px
    border-radius 50%
