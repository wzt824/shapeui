#box
  width 100%
  margin 10px auto
ul li
  list-style: none
  float: left
  border:1px solid #999999
  margin: 10px 10px 0
  width: 115px
  height: 40px
  line-height: 40px
  text-align: center
  border-radius: 5px
  background: #FF4007
  cursor: pointer
#box2
  width: 100%
  height: 100px
  border: 1px solid #999
  clear: both
  text-align: center
  line-height: 100px
.fix
  background: white
  border-bottom: white
  color: #000
#box2 .one
  display: block
#box2 div
  display: none
