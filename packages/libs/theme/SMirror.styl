#box
  position: relative
  margin:0 auto
  background-repeat no-repeat
  &:hover
    cursor move
#center
  position: relative
  width: 400px
  margin 0 auto
#center i:hover
  color: rgb(41, 0, 204)
#contains
  padding: 0 10px
  width: 380px
  height: 100px
  position: relative
  overflow: hidden

#center i
  font-size: 30px
  z-index: 2

#uls
  width: 600px
  padding: 20px 0
  position: absolute
  left: 0
  top: 0

#uls li
  width: 89px
  height: 60px
  border:1px solid white
  list-style: none
  float: left
  background-size:88px 60px

#uls li:hover
  border:1px solid red

.icon-jiantou1
  position: absolute
  right -20px
  top 25px
.icon-jiantou
  position: absolute
  left -40px
  top 25px
