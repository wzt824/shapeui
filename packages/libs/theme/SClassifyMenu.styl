ul
  list-style: none

a
  text-decoration: none

i
  font-style: normal
//导航下拉菜单
.containor
  width: 84%
  position: relative

.nav_left
  width:30%
  height: 100%
  background: rgba(85,138,197,.8)
  position: relative
  text-align center

.nav_left ul li
  height: 65px
  line-height 65px

.nav_left ul li a
  color: #fff
  font-size: 14px

.nav_left ul li .a
  display: block

.nav_left ul li i
  color: #fff
  float: right

.nav_right
  width:84%
  height: 100%
  position: absolute
  top: 0
  left: 30%
  background: rgba(14,18,43,0.8)
  overflow: hidden
  transition: all 0.8s
  display: none
  text-align center
  padding-top 100px
  a
    font-size 14px
    color #fff
    display block

.fix
  background: #9ebde1

.hides
  display: none
