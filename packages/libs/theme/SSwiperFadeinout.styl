#box2
  position:relative
  width:84%
  height:200px
  margin 0 auto
.swiper-side2
  position:absolute
  left:0
  top:0
  width:100%
  height:200px
  opacity:0
  background #1b6d85
.swiper-side2:nth-child(1)
  opacity:1
ul
  position:absolute
  right:50px
  bottom:10px
  list-style:none
  z-index:2
ul li
  float:left
  margin-left:20px
  width:10px
  height:10px
  border-radius:50%
  background-color:pink
li:nth-child(1)
  background-color:red
